{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended", "prettier"],
  "plugins": ["import", "react-hooks"],
  "rules": {
    "react/prop-types": "off",
    "react/display-name": "off"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
